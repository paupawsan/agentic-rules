  "agentic_bootstrap": {
    "version": "1.1.0",
    "framework_id": "agentic-core-rules-v1",
    "agent_algorithm_reference": "Bootstrap.md",
    "distributed_integration": true,
    "integration_model": "distributed_per_rule",
    "user_consent_required": true,
    "settings_confirmation_mandatory": true,
    "entry_points": {
      "global_config": "settings/global-settings.json",
      "memory_config": "modules/memory-rules/settings.json",
      "thinking_config": "modules/critical-thinking-rules/settings.json",
      "rag_config": "modules/rag-rules/settings.json",
      "agent_interaction_unit_test_config": "modules/agent-interaction-unit-test/settings.json"
    },
    "loading_sequence": [
      {
        "step": 0,
        "action": "check_initialization_status",
        "description": "Check if framework has been previously initialized",
        "required": true,
        "initialization_marker": ".agentic_initialized",
        "skip_if_initialized": true
      },
      {
        "step": 1,
        "action": "user_consent_prompt",
        "description": "Request user permission to enable Agentic Rules Framework",
        "required": true,
        "condition": "not_initialized",
        "user_prompt": "The Agentic Rules Framework enhances AI behavior with systematic reasoning, memory, and context management. Do you want to enable this framework? (y/n)"
      },
      {
        "step": 2,
        "action": "load_global_config",
        "file": "settings/global-settings.json",
        "required": true
      },
      {
        "step": 3,
        "action": "user_settings_confirmation",
        "description": "Confirm loaded settings with user before proceeding",
        "required": true,
        "confirmation_prompt": "Framework settings loaded. Review and confirm:\n- Memory Rules: {memory_enabled}\n- Critical Thinking Rules: {thinking_enabled}\n- RAG Rules: {rag_enabled}\n\nProceed with these settings? (y/n/modify)"
      },
      {
        "step": 4,
        "action": "load_rag_config",
        "file": "modules/rag-rules/settings.json",
        "condition": "global_config.agentic_rules_framework.rule_categories.rag_rules.enabled",
        "priority": "high",
        "user_approval_required": true,
        "approval_prompt": "RAG Rules will optimize context usage and enhance information retrieval. Enable? (y/n)"
      },
      {
        "step": 5,
        "action": "load_memory_config",
        "file": "modules/memory-rules/settings.json",
        "condition": "global_config.agentic_rules_framework.rule_categories.memory_rules.enabled",
        "priority": "medium",
        "user_approval_required": true,
        "approval_prompt": "Memory Rules will record interactions and integrate with RAG for enhanced context. Enable? (y/n)"
      },
      {
        "step": 6,
        "action": "load_thinking_config",
        "file": "modules/critical-thinking-rules/settings.json",
        "condition": "global_config.agentic_rules_framework.rule_categories.critical_thinking_rules.enabled",
        "priority": "high",
        "user_approval_required": true,
        "approval_prompt": "Critical Thinking Rules will validate responses and prevent hallucinations. Enable? (y/n)"
      },
      {
        "step": 6.5,
        "action": "load_agent_interaction_unit_test_config",
        "file": "modules/agent-interaction-unit-test/settings.json",
        "condition": "global_config.agentic_rules_framework.rule_categories.agent_interaction_unit_test.enabled",
        "priority": "low",
        "user_approval_required": true,
        "approval_prompt": "Agent Interaction Unit Test will enable automated validation and testing framework. Enable? (y/n)"
      },
      {
        "step": 7,
        "action": "initialize_orchestrator",
        "dependencies": ["global_config", "enabled_rule_configs"],
        "algorithm": "priority_based_rule_orchestration"
      }
    ],
    "rule_interconnections": {
      "error_to_memory": {
        "source_rule": "critical_thinking",
        "source_output": "error_admission_events",
        "target_rule": "memory",
        "target_input": "user_interaction_storage",
        "trigger_condition": "error_admitted",
        "data_mapping": {
          "error_content": "interaction_content",
          "correction": "interaction_outcome",
          "timestamp": "interaction_timestamp"
        }
      },
      "rag_to_memory_retrieval": {
        "source_rule": "rag",
        "source_output": "optimized_context_chunks",
        "target_rule": "memory",
        "target_input": "context_enhancement",
        "trigger_condition": "memory_retrieval_request",
        "data_mapping": {
          "relevant_chunks": "context_data",
          "relevance_scores": "priority_weights"
        }
      },
      "rag_to_memory_design": {
        "source_rule": "rag",
        "source_output": "pattern_analysis",
        "target_rule": "memory",
        "target_input": "design_guidance",
        "trigger_condition": "memory_construction_request",
        "data_mapping": {
          "similar_patterns": "design_templates",
          "context_relevance": "construction_priorities"
        }
      },
      "session_memory_to_rag": {
        "source_rule": "memory",
        "source_output": "session_topic_retrieval",
        "target_rule": "rag",
        "target_input": "context_optimization",
        "trigger_condition": "rag_context_request",
        "data_mapping": {
          "session_context": "context_data",
          "topic_history": "context_metadata"
        }
      },
      "ground_check_to_rag": {
        "source_rule": "critical_thinking",
        "source_output": "verification_results",
        "target_rule": "rag",
        "target_input": "quality_assurance",
        "trigger_condition": "information_validation_request",
        "data_mapping": {
          "verification_status": "quality_score",
          "source_reliability": "trust_level"
        }
      },
      "user_patterns_to_thinking": {
        "source_rule": "memory",
        "source_output": "user_pattern_analysis",
        "target_rule": "critical_thinking",
        "target_input": "assumption_challenge",
        "trigger_condition": "assumption_detected",
        "data_mapping": {
          "user_preferences": "context_factors",
          "interaction_history": "behavioral_patterns"
        }
      }
    },
    "platform_adapters": {
      "cursor": {
        "config_path": "settings/cursor-adapter.json",
        "tool_mapping_path": "cursor_tools.json",
        "memory_base_path": "${workspace}/agentic-memory",
        "supported_features": ["file_system", "terminal", "editor_tools"]
      },
      "vscode": {
        "config_path": "settings/vscode-adapter.json",
        "tool_mapping_path": "vscode_tools.json",
        "memory_base_path": "${workspace}/.agentic/memory",
        "supported_features": ["file_system", "terminal", "extension_api"]
      },
      "ci_system": {
        "config_path": "settings/ci-adapter.json",
        "tool_mapping_path": "ci_tools.json",
        "memory_base_path": "/tmp/agentic-memory",
        "supported_features": ["file_system", "command_execution", "environment_vars"]
      }
    },
    "framework_validation": {
      "required_config_files": [
        "settings/global-settings.json",
        "modules/memory-rules/settings.json",
        "modules/critical-thinking-rules/settings.json",
        "modules/rag-rules/settings.json",
        "modules/agent-interaction-unit-test/settings.json"
      ],
      "required_rule_files": [
        "modules/memory-rules/MEMORY-RULES.md",
        "modules/critical-thinking-rules/CRITICAL-THINKING-RULES.md",
        "modules/rag-rules/RAG-RULES.md",
        "modules/agent-interaction-unit-test/AGENTS.md"
      ],
      "bootstrap_compliance": {
        "loading_sequence_mandatory": true,
        "user_consent_enforced": true,
        "settings_confirmation_required": true,
        "rule_interconnections_respected": true,
        "platform_adapters_applied": true,
        "compliance_violation_logging": true,
        "bootstrap_override_prohibited": true
      },
      "validation_checks": [
        "json_syntax_validation",
        "rule_dependency_resolution",
        "platform_compatibility_check",
        "version_compatibility_check",
        "bootstrap_compliance_verification"
      ],
      "validation_schema": {
        "config_format": "json",
        "rule_format": "markdown",
        "encoding": "utf-8"
      }
    },
    "runtime_orchestration": {
      "rule_orchestrator": {
        "input_interfaces": ["user_request", "tool_capabilities", "system_state"],
        "output_interfaces": ["rule_activation_signals", "tool_execution_requests", "response_synthesis"],
        "execution_model": "priority_based",
        "conflict_resolution": "rule_priority_hierarchy",
        "monitoring_enabled": true
      },
      "transparency_system": {
        "log_format": "markdown",
        "log_base_path": "logs/rule-applications/",
        "retention_policy_days": 30,
        "compression_enabled": true
      }
    }
  }
}
